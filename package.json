    "@radix-ui/react-toast": "^1.2.7",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "connect-pg-simple": "^10.0.0",
    "dotenv": "^17.0.1",
    "drizzle-orm": "^0.39.1",
    "drizzle-zod": "^0.7.0",
